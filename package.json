{
  "name": "keyframe-x",
  "version": "1.0.1",
  "keywords": [
    "keyframe",
    "tween",
    "animate",
    "object",
    "array",
    "deep",
    "number",
    "string"
  ],
  "main": "dist/index.js",
  "browser": "dist-browser/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/keyframe-x.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/inject.js' --browser",
    "build": "esca-scripts build --input src --output dist --babel --multiple",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --browser",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "npm run build && esca-scripts test",
    "travis": "opn https://travis-ci.org/escaladesports/keyframe-x/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "clone": "^1.0.4",
    "string-tween": "^0.1.0"
  }
}
